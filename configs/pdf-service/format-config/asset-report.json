{
  "key": "asset-report",
  "config": {
    "defaultStyle": {
      "font": "Cambay"
    },
    "content": [
      {
        "style": "noc-head",
        "table": {
          "widths": [85, "*", 120],
          "body": [
            [
              {
                "image": "{{logoImage}}",
                "width": 70,
                "height": 40,
                "margin": [5, 10, 0, 0]
              },
              {
                "stack": [
                  {
                    "text": "{{logo-header}} {{Municipal_Corporation}}",
                    "style": "receipt-logo-header",
                    "alignment": "left",
                "fontSize": 14,
                "bold": true,
                "margin": [5, 5, 0, 0]
                  },
                  
                  {
                    "text": "{{asset_receipt}}",
                    "style": "receipt-logo-sub-header",
                    "alignment": "left",
                    "fontSize": 16,
                    "bold": true,
                    "decoration": "underline",
                    "margin": [5, 5, 0, 0]
                  }
                ]
              },
              {
                "stack": [
                  {
                    "text": "{{address}}"
                  },
                  {
                    "text": "{{website}}"
                  },
                  {
                    "text": "{{email}}, {{phoneNumber}}"
                  }
                ],
                "alignment": "right",
                "color": "#484848",
                "margin": [-100, 0, -15, 0]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "style": "noc-table",
        "table": {
          "widths": ["22%", "28%", "22%", "28%"],
          "body": [
            [
              {
                "text": "{{asset_book_ref_no}}",
                "border": [true, true, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetBookRefNo}}",
                "border": [false, true, false, false],
                "style": "receipt-table",
                "font": "Roboto"
              },
              {
                "text": "{{asset_id}}",
                "border": [false, true, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{applicationNo}}",
                "border": [false, true, true, false],
                "style": "receipt-table",
                "font": "Roboto"
              }
            ],
            [
              {
                "text": "{{asset_creation_date}}",
                "border": [true, false, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetCreationDate}}",
                "border": [false, false, false, false],
                "style": "receipt-table"
              },
              {
                "text": "{{asset_name}}",
                "border": [false, false, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetName}}",
                "border": [false, false, true, false],
                "style": "receipt-table"
              }
            ],
            [
              {
                "text": "{{asset_status}}",
                "border": [true, false, false, true],
                "style": "receipt-table-value"
              },
              {
                "text": "{{status}}",
                "border": [false, false, false, true],
                "style": "receipt-table"
              },
              {
                "text": "{{asset_current_usage}}",
                "border": [false, false, false, true],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetCurrentUsage}}",
                "border": [false, false, true, true],
                "style": "receipt-table"
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "next-noc-table",
        "table": {
          "widths": ["22%", "28%", "22%", "28%"],
          "body": [
            [
              {
                "text": "{{asset_classification}}",
                "border": [true, true, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetClassification}}",
                "border": [false, true, false, false],
                "style": "receipt-table"
              },
              {
                "text": "{{asset_parent_category}}",
                "border": [false, true, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": " {{assetParentCategory}}",
                "border": [false, true, true, false],
                "style": "receipt-table"
              }
            ],
            [
              {
                "text": "{{asset_category}}",
                "border": [true, false, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{assetCategory}}",
                "border": [false, false, false, false],
                "style": "receipt-table"
              },
              
              {
                "text": "{{tenant_id}}",
                "border": [false, false, false, false],
                "style": "receipt-table-value"
              },
              {
                "text": "{{City}}",
                "border": [false, false, true, false],
                "style": "receipt-table",
                "font": "Roboto"
              }
            ],
            [
              {
                "text": "{{asset_description}}.",
                "border": [true, false, false, true],
                "style": "receipt-table-value"
              },
              {
                "text": "{{description}}",
                "border": [false, false, false, true],
                "style": "receipt-table"
              },
              {
                "text": "{{asset_remarks}}",
                "border": [false, false, false, true],
                "style": "receipt-table-value"
              },
              {
                "text": "{{remarks}}",
                "border": [false, false, true, true],
                "style": "receipt-table"
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "next-noc-table",
        "table": {
          "widths": ["25%", "25%", "25%", "25%"],
          "body": [
            [
              {
                "table": {
                  "body": [
                    [
                      {
                        "text": [
                          { "text": "Purchase Order Number: ", "bold": true },
                          { "text": "{{assetPurchaseOrderNumber}}", "bold": false }
                        ],
                        "border": [false, false, false, false]
                      }
                    ]
                  ]
                },
                "border": [true, true, true, true], 
                "style": "receipt-table-value-down"
              },
              {
                "table": {
                  "body": [
                    [
                      {
                        "text": [
                          { "text": "Book Value: ₹ ", "bold": true },
                          { "text": "{{assetBookValue}}", "bold": false },
                          { "text": "/-", "bold": true }
                        ],
                        "border": [false, false, false, false]
                      }
                    ]
                  ]
                },
                "border": [true, true, true, true], 
                "style": "receipt-table-value-down"
              },
              {
                "table": {
                  "body": [
                    [
                      {
                        "text": [
                          { "text": "Acquisition Cost: ₹ ", "bold": true },
                          { "text": "{{assetAcquisitionCost}}", "bold": false },
                          { "text": "/-", "bold": true }
                        ],
                        "border": [false, false, false, false]
                      }
                    ]
                  ]
                },
                "border": [true, true, true, true], 
                "style": "receipt-table-value-down"
              },
              {
                "table": {
                  "body": [
                    [
                      {
                        "text": [
                          { "text": "Purchase Cost: ₹ ", "bold": true },
                          { "text": "{{assetPurchaseCost}}", "bold": false },
                          { "text": "/-", "bold": true }
                        ],
                        "border": [false, false, false, false]
                      }
                    ]
                  ]
                },
                "border": [true, true, true, true], 
                "style": "receipt-table-value-down"
              }
            ]
            
          ]
        },
        "layout": {}
      },      
      {
        "style": "next-noc-table",
        "table": {
          "widths": ["60%", "30%", "10%"],
          "body": [
            [
              {
                "table": {
                  "body": [
                    [
                      {
                        "text": "{{asset_address}}",
                        "bold": true,
                        "border": [false, false, false, false]
                      }
                    ],
                    [
                      {
                        "text": "{{addressLine1}}, {{addressLine2}},",
                        "border": [false, false, false, false]
                      }
                    ],
                    [
                      {
                        "text": "{{City}}",
                        "border": [false, false, false, false]
                      }
                    ]
                  ]
                },
                "border": [true, true, false, true],
                "style": "receipt-table-value-down"
              },
              {
                "text": "",
                "border": [false, true, false, true],
                "bold": true,
                "style": "receipt-table-value-down"
              },
              {
                "image": "{{qrcodeimage}}",
                "style": "receipt-table-value-total-Qr",
                "border": [false, true, true, true],
                "width": 72,
                "height": 72
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "receipt-approver",
        "columns": [
          {
            "text": [
              {
                "text": "{{generated_by}}: ",
                "bold": true
              },
              {
                "text": "{{user-name}}",
                "bold": false
              }
            ],
            "alignment": "left"
          },
          {
            "text": [
              {
                "text": "{{commissioner/eo}}",
                "bold": true
              }
            ],
            "alignment": "right",
            "margin": [0, 0, -10, 1]
          }
        ]
      },
      {
        "style": "no-signature",
        "text": "{{no_signature}}"
      }
    ],
    "styles": {
      "noc-head": {
        "margin": [-30, -30, 0, 1]
      },
      "receipt-logo-header": {
        "color": "#000000",
        "fontSize": 14,
        "bold": true,
        "letterSpacing": 0.74,
        "margin": [0, 0, 0, 5]
      },
      "receipt-logo-sub-header": {
        "color": "#000000",
        "fontSize": 12,
        "letterSpacing": 0.6
      },
      "noc-table": {
        "fontSize": 12,
        "color": "#000000",
        "margin": [-25, 10, -22, -8]
      },
      "next-noc-table": {
        "fontSize": 12,
        "color": "#000000",
        "margin": [-25, 28, -22, -8]
      },
      "receipt-table-value": {
        "color": "#000000",
        "bold": true,
        "fontSize": 12,
        "fontWeight": 500,
        "margin": [8, 2, 0, 2]
      },
      "receipt-table-value-total-Qr": {
        "alignment": "right",
        "margin": [0, 3, 10, 0]
      },
      "receipt-table-value-down": {
        "color": "#000000",
        "bold": false,
        "fontSize": 12,
        "fontWeight": 400,
        "alignment": "left",
        "margin": [3, 5, 0, 5]
      },
      "receipt-table": {
        "color": "#000000",
        "bold": false,
        "fontSize": 12,
        "fontWeight": 400,
        "margin": [-10, 2, 0, 2]
      },
      "receipt-approver": {
        "fontSize": 12,
        "bold": true,
        "margin": [-27, 50, -10, 0],
        "color": "#484848"
      },
      "no-signature": {
        "fontSize": 12,
        "margin": [-27, 10, -10, 0],
        "color": "#484848"
      },
      "pt-disclaimer": {
        "fontSize": 12,
        "margin": [-27, 10, -10, 0],
        "color": "#484848"
      },
      "pt-disclaimer-value": {
        "fontSize": 12,
        "margin": [2, 3, 0, 0],
        "color": "#484848"
      }
    }
  }
}
